          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@scaleImg)": {
              ".card .card-img": {
                "overflow": "hidden",
                "img": {
                  "transition": "all .5s"
                },
                "&:hover": {
                  "img": {
                    "transform": "scale3d(1.1, 1.1, 1.1)"
                  }
                }
              }
            },
            ".underline": {
              ".line": {
                "width": "2rem",
                "height": "1px",
                "background": "@underlineColor",
                "display": "inline-block"
              }
            },
            ".card": {
              "height": "100%",
              ".card-wrapper": {
                "height": "100%",
                ".card-img": {
                  "border-radius": "0"
                }
              },
              ".card-box": {
                "background": "@cardBg",
                "padding": "2rem",
                ".card-title": {
                  "margin": "0"
                }
              },
              ".info-wrapper": {
                "display": "inline-flex",
                "align-items": "center",
                ".mbr-iconfont": {
                  "padding-right": ".5rem",
                  "font-size": "20px"
                },
                ".info-text": {
                  "display": "inline-block",
                  "padding-right": "1rem",
                  "margin": "0"
                }
              },
              ".card-btn .btn": {
                "margin-left": "4px !important",
                "margin-right": "4px !important"
              },
              ".card-footer": {
                "background": "@cardBg",
                "margin-top": "1rem",
                "padding": "0",
                "padding-top": "1rem",
                "display": "inline-flex",
                "width": "100%",
                "justify-content": "space-between",
                "align-items": "center",
                ".icons-list": {
                  "margin": "0",
                  "li": {
                    "margin-right": "1rem",
                    ".mbr-iconfont": {
                      "font-size": "25px",
                      "color": "#767676"
                    }
                  }
                },
                ".link": {
                  ".link-text": {
                    "display": "inline-block"
                  },
                  ".mbr-iconfont": {
                    "font-size": "12px"
                  }
                }
              }
            },
            "& when (@showBorders)": {
              ".card-box": {
                "border-style": "solid",
                "border-color": "@bordersColor",
                "border-width": "0 1px 1px 1px"
              },
              ".card-footer": {
                "border-top": "1px solid @bordersColor"
              }
            },
            "& when not (@showBorders)": {
              ".card-footer": {
                "border": "none"
              }
            },
            ".mbri-arrow-next": {
              "color": "#709faf"
            },
            "@media (max-width: 767px)": {
              ".card:not(:last-child)": {
                "padding-bottom": "2rem"
              }
            },
            "@media (max-width: 991px)": {
              ".card:not(:last-child)": {
                "padding-bottom": "2rem"
              }
            }
          },
